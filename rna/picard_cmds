samtools merge -@ 8 -o rna/STAR/Treg.4.FP3pl.Nrplo.Co_merge.bam rna/STAR/Treg.4.FP3pl.Nrplo.Co_1Aligned.sortedByCoord.out.bam rna/STAR/Treg.4.FP3pl.Nrplo.Co_2Aligned.sortedByCoord.out.bam ; samtools view -@8 -hf 2 -q 10 rna/STAR/Treg.4.FP3pl.Nrplo.Co_merge.bam | awk '{if ($3 != "chrM" && $3 != "chrY") print}' | samtools view -b -@8 | bedtools intersect -v -wa -a stdin -b comb_badRegions.bed > rna/STAR/Treg.4.FP3pl.Nrplo.Co_tmp.bam; java -jar /programs/picard-tools-2.19.2/picard.jar MarkDuplicates I=rna/STAR/Treg.4.FP3pl.Nrplo.Co_tmp.bam ASSUME_SORT_ORDER=coordinate O=rna/STAR/Treg.4.FP3pl.Nrplo.Co.dedup.bam M=rna/STAR/Treg.4.FP3pl.Nrplo.Co.metrics REMOVE_DUPLICATES=true
